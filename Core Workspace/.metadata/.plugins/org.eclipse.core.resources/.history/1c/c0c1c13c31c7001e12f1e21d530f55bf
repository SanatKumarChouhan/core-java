package Practice;

public class TestShape extends Triangle {
	
	public static void main(String[] args) {
		
		Triangle t=new Triangle();
		
		t.setColor("Mat Gray");
		t.setBorderWidth(26);
		
		t.setBase(10);
		t.setHeight(20);
		
		System.out.println(t.getBase());
		System.out.println(t.getHeight());
		
		t.area();
		
		
		
		
		
	}
	
	

}
