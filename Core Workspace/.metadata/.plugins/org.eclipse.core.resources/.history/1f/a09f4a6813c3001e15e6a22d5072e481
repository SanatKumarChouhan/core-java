package in.co.rays.basic;

public class AinvayiStringMethod {
	
	public static void main(String[] args) {
		
		String name="mission is to train";
		
		System.out.println(name.length());
		
		System.out.println(name.charAt(6));
		
		System.out.println(name.indexOf("t"));
		
		System.out.println(name.indexOf(" is "));
	    
		System.out.println(name.lastIndexOf("o"));
		
		System.out.println(name.replace("i", "o"));
		
		System.out.println(name.toUpperCase());
		
		System.out.println(name.toLowerCase());
		
		System.out.println(name.startsWith("mi"));
		System.out.println(name.endsWith("on"));

}
}