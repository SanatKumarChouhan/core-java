package Practice;

public class E2exception {
	
	public static void main(String[] args) {
		
		try {
			
			String name="Shubham Shandilya";
			
			System.out.println(name.length());
			System.out.println(name.charAt(15));
			System.out.println(name.indexOf("Sha0"));
			System.out.println(name.indexOf("S"));
			System.out.println(name.lastIndexOf(0));
			System.out.println(name.startsWith("Shu"));
			System.out.println(name.substring(6));
			
			
		}catch (Exception e) {
			
			//System.out.println(e);
			e.printStackTrace();
		}
		
		
	}

}
