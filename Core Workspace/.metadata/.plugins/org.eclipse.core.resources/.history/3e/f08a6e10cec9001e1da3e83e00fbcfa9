package in.co.rays.basic;

public class PrimeNumber {

	public static void main(String[] args) {
		
	int num=15;
	int count=0;
	
	for (int i = 2; i <= num; i++) {
		
		if (num%i==0) {
			count++;
		}
		
	}
if (count==3) {
	System.out.println("yes prime number");
	
}else {
	System.out.println("no prime number");
}
	
	
	}

}
