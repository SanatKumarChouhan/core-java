package Practice;

public class Sachin {
	
	public static void main(String[] args) {
		
		Bittu b=new Bittu();
		
		b.setBittuName("shubham lawvanshi");
		b.setBittuFather("suresh lawvanshi");
		b.setBittuMother("chanda lawvanshi");
		
		System.out.println(b.getBittuName());
		System.out.println(b.getBittuFather());
		System.out.println(b.getBittuMother());
		
		
	}

}
