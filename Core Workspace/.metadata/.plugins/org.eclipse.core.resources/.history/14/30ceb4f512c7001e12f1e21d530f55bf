package in.co.rays.inheritance;

public class TestShape extends Triangle {
	
	public static void main(String[] args) {
		
		TestShape t=new TestShape();
		
		t.setColor("blue");
		
		t.setBorderwidth(23);
		
		System.out.println(t.getColor());
		
		System.out.println(t.getBorderwidth());
		
		
		
		
		
		
	}

}
