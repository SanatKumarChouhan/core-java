package Practice;

public class PPradeep {
	// by using method
	
	public void run() {
		System.out.println("Running is a good habbit for human health");
	}
	
	public void eat() {
		System.out.println("Everyone have a good diet");
	}
	
	public void sum(int a, int b) {
		int plus=a+b;
		System.out.println(plus);
	}
	
	public void PrimeNumber(int n) {
		if (n%2==0) {
			System.out.println("this is not your Prime number");
		}else {
			System.out.println("this is your Prime number");
		}
		
	}
	
	public void whoisgreather(int a, int b) {
		if (a==b) {
			System.out.println("both no. are equal");
		}else if(a>b){
			System.out.println("a is greather");
		}else {
			System.out.println("a is lesser");
		}
	}
	
	
	
}
	
