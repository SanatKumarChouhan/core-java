package Practice;

public class Bittu {

	private String bittuname;
	private String bittufather;
	private String bittumother;
	
	public void setBittuName(String bittuname) {
		this.bittuname=bittuname;
	}
	public String getBittuName() {
		return bittufather;
	}
	
	public void setBittuFather(String bittufather) {
		this.bittufather=bittufather;
	}
	
	public String getBittuFather() {
		return bittufather;
	}
	
	public void setBittuMother(String bittumother) {
		this.bittumother=bittumother;
	}
	public String getBittuMother() {
		return bittumother;
	}
		
	
}
