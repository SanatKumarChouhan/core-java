package Practice;

public class TypeCasting {
	
	public static void main(String[] args) {
		
		
		byte b=5;
		double d=b;
		
		System.out.println("byte to double = "+d);
		
		double d1=5.5;
		byte b1=(byte)d1;
		
		System.out.println(b1);
		
		float f=25;
		
		System.out. println(f);
		
		short s=30;
		float t=(float)s;
		
		System.out.println(t);
		
		
		
		
	}

}
